<% friends_with_channels.each do |friend_with_channels| %>
    <% friend = friend_with_channels[:friend] %>
    <div class="friend-box-with-edit">
        <button class="btn-friend-channel-edit">-</button>
        <div class="friend-box <%=friend.id%>">
            <div class="friend-box-userprofile">
                <%= link_to image_tag(friend.image.url, class:"friend-box-image"), profile_path(friend.id) %>
                <span class = "user-name" ><%= friend.username %></span>
            </div>
            <button data-url="<%=add_friend_path(friend.id)%>" class="btn-friend-remove" >친구 끊기</button>
            <%friend_with_channels[:channels].each do |channel| %>
                <span class="channel-id <%= channel.id%>"></span>
            <% end %>
        </div>
    </div>
<% end %>