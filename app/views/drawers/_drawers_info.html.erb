<div>
    친구 <%=friends_count%>명 / 익명 <%=anonymous_count %>명이 서랍장
    <hr>
    <% users.each do |user| %>
        <div>
            <%= link_to image_tag(user[:image_url], :style => "height:40px; width:40px; border-radius:20px; margin-right: 5px;"), user[:profile_path] %>
            <%= link_to user[:username], user[:profile_path], class: "username" %>
        </div>    
    <% end %>
</div>