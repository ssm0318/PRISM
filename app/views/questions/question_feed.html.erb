Question Feed
<% @question.answers.each do |answer| %>
  <div style="border: 1px solid black;">
    <div><%= answer.author.email %></div>
    <div><%= answer.question.content %></div>
    <div><%= answer.content %></div>
    <div><%= answer.updated_at %></div>
  </div>
<% end %>

