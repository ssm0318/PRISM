
<%- if devise_mapping.confirmable? && (controller_name != 'confirmations') %>
  <%= link_to "이메일 인증 메일을 받지 못하셨나요?", new_confirmation_path(resource_name) %><br />
<% end -%>

<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to image_tag("icons/kakao.png"), omniauth_authorize_path(resource_name, provider) %><br />
  <% end -%>
<% end -%>

