<% feeds.each do |feed| %>
    <div class="prism-box">
        <div class="prism-box__header">
            <%= render partial: "./user_profile_general", locals: {user_id: feed.author_id, feed_id: feed.id} %>
        </div>
        <div class="prism-box__content"><br>
            <a href="/questions/<%=answer.question.id%>/friend" class="question" style="font-size: 18px;"><%= answer.question.content %></a> <br><br>
            <div class="answer"><%= answer.content %> <br><br></div>
            <div class = "date"><%= time_tag answer.updated_at, class: "timeago" %></div>
        </div>
        <div class="prism-box__footer" style="padding-bottom: 10px;">
            <%= render 'likes/general', a: answer, target_type: "Answer", like_btn_class: "btn-answer-like" %>
            <%= render 'drawer', a: answer , target_type: "Answer" %> 
            <br>
        </div> <!--이거 처음 쓴 날짠지 업데이트한 날짠지..?-->
        <%= render 'comment_general_form', answer: answer %>
    </div>
<% end %>  